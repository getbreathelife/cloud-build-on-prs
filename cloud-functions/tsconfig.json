{
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "target": "es2017",
    "strict": true,
    "resolveJsonModule": true,
    "sourceMap": true,
  }
}
